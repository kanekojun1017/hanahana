<h1>投稿</h1>
<%= form_with model: @post do |f| %>
 <H4>投稿</H4>
 <%= f.text_field :body %><br>
 <%= text_field_tag :zipcode, nil, placeholder: "郵便番号" %><br>
 <%= f.text_field :address, placeholder: "住所" %><br>
 <%= f.submit '投稿' %>
<% end %>
<%= javascript_pack_tag "jpostal",  'data-turbolinks-track': 'reload' %>