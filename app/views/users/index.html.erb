<h1>ユーザー一覧</h1>
<table>
    <thead>
      <tr>
        <th>ユーザー画像</th>
        <th>名前</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    
    <tbody>
    <% @users.each do |user| %>
        <tr>
          <td>
            <%= link_to user_path(user.id) do %>
              <%= image_tag user.get_profile_image(80, 80)%>
            <% end %>
            </td>
          <td><%= user.name%></td>
        </tr>
    <% end %>
    </tbody>
</table>