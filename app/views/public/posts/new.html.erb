<h1>投稿ページ</h1>
<%= form_with model: @post do |f| %>
 <H4>投稿</H4>
 <%= f.file_field :image, accept: "image/*" %>
 <%= f.text_area :body, placeholder: "説明" %>
 <%= f.text_field :address, id: "address", placeholder: "住所" %>
 <%= f.submit '投稿' %>
<% end %>
<div id="map"></div>
<%= javascript_pack_tag 'map', 'data-turbolinks-track': 'reload' %>